                                <label className="switch switch-text switch-warning switch-pill"><input type="checkbox" className="switch-input"/> <span data-on="On" data-off="Off" className="switch-label"></span> <span className="switch-handle"></span></label>

                                <label className="switch switch-text switch-info switch-pill"><input type="checkbox" className="switch-input"/> <span data-on="On" data-off="Off" className="switch-label"></span> <span className="switch-handle"></span></label>

                                <label className="switch switch-text switch-danger switch-pill"><input type="checkbox" className="switch-input"/> <span data-on="On" data-off="Off" className="switch-label"></span> <span className="switch-handle"></span></label>

                            </div>
                        </div>
                    </div>{/* /.col */}

                    <div className="col-md-12">
                        <div className="card">
                            <div className="card-header">
                                <strong v-if="headerText" className="card-title">Sizes</strong>
                            </div>
                            <div className="card-body p-0">
                                <table className="table table-hover table-striped table-align-middle mb-0">
                                    <thead>
                                        <tr><th>Size</th>
                                        <th>Example</th>
                                        <th>CSS Class</th>
                                    </tr></thead>
                                    <tbody>
                                        <tr>
                                            <td>
                                                Large
                                            </td>
                                            <td>
                                                <basix-switch variant="primary" :checked="true" type="3d" size="lg">
                                            </basix-switch></td>
                                            <td>
                                                Add following code <code>size="lg"</code>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                Default
                                            </td>
                                            <td>
                                                <basix-switch variant="primary" :checked="true" type="3d">
                                            </basix-switch></td>
                                            <td>

                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                Small
                                            </td>
                                            <td>
                                                <basix-switch variant="primary" :checked="true" type="3d" size="sm">
