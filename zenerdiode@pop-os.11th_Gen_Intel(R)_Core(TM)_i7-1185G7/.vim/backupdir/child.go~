package main

import (
    "fmt"
    "os/exec"
    "bytes"
)

func main() {
    // Define the command to run
    cmd := exec.Command("python", "../python/win.py")

    // Get the output of the command
    var out bytes.Buffer
    cmd.Stdout = &out

    // Run the command
    err := cmd.Run()
    if err != nil {
        fmt.Println("Error:", err)
        return
    }

    // Print the output
    fmt.Println(out.String())
}
